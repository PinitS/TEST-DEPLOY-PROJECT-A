{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@controllers/*": ["./src/controllers/*"],
      "@database/*": ["./src/database/*"],
      "@middleware/*": ["./src/middleware/*"],
      "@redis/*": ["./src/redis/*"],
      "@routes/*": ["./src/routes/*"],
      "@socket/*": ["./src/socket/*"],
      "@static/*": ["./src/static/*"],
      "@utils/*": ["./src/utils/*"],
      "@validate/*": ["./src/validate/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
